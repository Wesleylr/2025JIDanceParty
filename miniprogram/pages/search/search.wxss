page {
  padding: 20rpx;
}

.backgroundImage {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-image: url("https://img2.imgtp.com/2024/03/28/7ImpsxSV.jpg");
  background-size: cover;
  background-position: 0rpx -900rpx;
  z-index: -1;
}

.search_row {
  background-color: #fff;
  height: 70rpx;
  margin-right: 15px;
  margin-bottom: 10px;
  border-radius: 5px;
  display: flex;
}

.search_row input {
  flex: 1;
  height: 100%;
}

.search_row button {
  width: 75rpx !important;
  height: 100%;
  padding: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.first_tab .dancers .dancerImage .hasPartner {
  margin-top: 5px;
  font-size: 24rpx;
}

.first_tab .dancers .dancerImage .noPartner {
  margin-top: 5px;
  font-size: 24rpx;
}

/* pages/home/home.wxss */
.user_icon {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
}

.test {
  display: fixed;
  justify-content: center;
}

.search_input {
  height: 50rpx;
  padding: 10rpx;
  background-color: dodgerblue;
}

.search_input navigator {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: snow;
  border-radius: 15rpx;
  color: #666;
}

.first_tab {
  position: relative;
  margin-bottom: 20rpx;
}

.first_tab .corner {
  position: absolute;
  bottom: 2rpx;
  right: 40rpx;
  height: 60rpx;
  width: 60rpx;
}

.first_tab .dancers {
  display: flex;
  margin: 3rpx;
  box-shadow: 0 3px 6px rgba(0, 0, 0, 0.2);
  margin-right: 40rpx;
  background-color: rgba(255, 255, 255, 0.4);
  border-bottom-left-radius: 10px;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
}

.first_tab .dancers .dancerImage {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.first_tab .dancers .dancerImage image {
  width: 70%;
}

.first_tab .dancers .dancerInfo {
  flex: 2;

}

.first_tab .dancers .dancerInfo .dancerName {
  font-size: 27rpx;
}

.first_tab .dancers .dancerImage .hasPartner {
  margin-top: 5px;
  font-size: 24rpx;
}

.first_tab .dancers .dancerImage .noPartner {
  margin-top: 5px;
  font-size: 24rpx;
}

.first_tab .dancers .dancerInfo .dancerGender {
  font-size: 27rpx;
}

.first_tab .dancers .dancerInfo .dancerSchool {
  font-size: 27rpx;
}

.first_tab .dancers .dancerInfo .dancerMsg {
  font-size: 27rpx;
}

.first_tab .dancers .dancerInfo .dancerMsg2 {
  font-size: 27rpx;
  color: gray;
}

.tips {
  display: flex;
  align-items: center;
  justify-content: center;
  color: gray;
  height: 800rpx;
}